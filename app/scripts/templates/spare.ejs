<h3 class="h5">Sticker ID # <%= data.id %></h3>

<div class="checkbox__container">
    <input type="checkbox" name="reserved-swap" class="reserved-swap checkbox checked-icon"
        <%= data.reserved ? 'checked="checked"' : '' %> />
    <label for="reserved-swap" class="reserved-swap checkbox__label">Reserved</label>
</div>

<div class="checkbox__container">
    <input type="checkbox" name="swapped" class="swapped checkbox checked-icon"
        <%= data.swapped ? 'checked="checked"' : '' %> />
    <label for="swapped" class="swapped checkbox__label">Swapped</label>
</div>

<div class="swapped-with">
    <label for="swapped-with">Swapped With</label>
    <input type="text" name="swapped-with" class="swapped-with__input" placeholder="Name or @Username" maxlength="32" />
    <button type="submit" class="submit">OK</button>
</div>
